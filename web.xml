<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Coffee</servlet-name>
        <servlet-class>com.example.web.CoffeeSelect</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CoffeeExpert</servlet-name>
        <servlet-class>com.example.model.CoffeeExpert</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Coffee</servlet-name>
        <url-pattern>/SelectCoffee.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CoffeeExpert</servlet-name>
        <url-pattern>/CoffeExpert</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>